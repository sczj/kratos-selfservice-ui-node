{
"$id":"https://schemas.ory.sh/presets/kratos/quickstart/email-password/identity.schema.json",
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"Person",
    "type":"object",
    "properties":{
        "email":{
            "type":"string",
            "format":"email",
            "title":"EMAIL",
            "minLength":3,
            "ory.sh/kratos":{
                "credentials":{
                    "password":{
                        "identifier":true
                    }
                }
            }
        },

      "mobile":{
            "type":"string",
            "title":"MOBILE",
            "minLength":3,
            "ory.sh/kratos":{
                "credentials":{
                    "password":{
                        "identifier":true
                    }
                }
            }
        }
    },
    "required":[
        "email",
"mobile"
    ],
    "additionalProperties":false
}















































